{
  "extends": "airbnb/base",
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "guard-for-in": 0,
    "func-names": 0,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "arrow-body-style": [
      2,
      "always"
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ]
  }
}
